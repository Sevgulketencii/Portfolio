@{
    Layout = "_AdminLayout";
}

<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="overview-wrap">
                        <h2 class="title-1">Dash-Board</h2>
                    </div>
                </div>
            </div>
            @await Component.InvokeAsync("_Widgets")

            <div class="row">
                @await Component.InvokeAsync("_ProjectTable")
                <div class="col-lg-3">
                    <h2 class="title-1 m-b-25">Sertifikalar</h2>
                    @await Component.InvokeAsync("_Certificate")
                </div>
            </div><br />

            <br />
            @await Component.InvokeAsync("_HomeContact") 

        </div>
    </div>
</div>
